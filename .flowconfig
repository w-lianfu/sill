[ignore]
.*/node_modules/styled-components/.*
.*/flow-typed/npm/.*

[include]
.*/src/.*

[libs]
.*/flow-typed/.*

[lints]

[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.name_mapper='.*\(.s?css\)' -> 'css-module-flow'
module.name_mapper='^@comp/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^@/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
esproposal.decorators=ignore
module.system=haste

[strict]
